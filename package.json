{
    "name": "threejs-demo",
    "version": "0.0.1",
    "private": true,
    "author": "v_lishaohai",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "npm run rd && npm run qa",
        "rd": "vue-cli-service build --mode rd",
        "qa": "vue-cli-service build --mode qa",
        "lint": "vue-cli-service lint",
        "start": "npm run serve",
        "dev": "npm run serve"
    },
    "dependencies": {
        "axios": "^0.19.0",
        "core-js": "^3.2.1",
        "element-ui": "^2.12.0",
        "three": "^0.108.0",
        "three-orbit-controls": "^82.1.0",
        "vue": "^2.6.10",
        "vue-router": "^3.1.3",
        "vuex": "^3.1.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.11.0",
        "@vue/cli-plugin-eslint": "^3.11.0",
        "@vue/cli-service": "^3.11.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "babel-eslint": "^10.0.3",
        "eslint": "^6.3.0",
        "eslint-plugin-vue": "^5.2.3",
        "less": "^3.10.3",
        "less-loader": "^5.0.0",
        "node-sass": "^4.12.0",
        "sass-loader": "7.1.0",
        "vue-template-compiler": "^2.6.10"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "@vue/standard"
        ],
        "rules": {
            "vue/no-parsing-error": [
                2,
                {
                    "x-invalid-end-tag": false
                }
            ],
            "no-regex-spaces": "error",
            "no-multi-spaces": "error",
            "array-bracket-spacing": [
                "error",
                "never"
            ],
            "object-curly-spacing": [
                "error",
                "never"
            ],
            "block-spacing": [
                "error",
                "never"
            ],
            "comma-spacing": [
                "error",
                {
                    "before": false,
                    "after": true
                }
            ],
            "semi-spacing": [
                "error",
                {
                    "before": false,
                    "after": true
                }
            ],
            "computed-property-spacing": [
                "error",
                "never"
            ],
            "no-trailing-spaces": "off",
            "no-spaced-func": "error",
            "space-before-function-paren": [
                "error",
                {
                    "anonymous": "ignore",
                    "named": "ignore"
                }
            ],
            "space-before-blocks": [
                "error",
                "always"
            ],
            "space-in-parens": [
                "error",
                "never"
            ],
            "space-infix-ops": [
                "error",
                {
                    "int32Hint": false
                }
            ],
            "space-unary-ops": "error",
            "spaced-comment": [
                "error",
                "always"
            ],
            "arrow-spacing": "error",
            "yield-star-spacing": [
                "error",
                {
                    "before": true,
                    "after": false
                }
            ],
            "no-irregular-whitespace": "error",
            "template-curly-spacing": [
                "error",
                "never"
            ],
            "max-len": [
                "error",
                120
            ],
            "no-multiple-empty-lines": "error",
            "eol-last": "off",
            "lines-around-comment": [
                "error",
                {
                    "beforeBlockComment": false
                }
            ],
            "curly": [
                "error",
                "multi-line"
            ],
            "camelcase": [
                "error",
                {
                    "properties": "never"
                }
            ],
            "no-unused-vars": 0,
            "arrow-parens": 0,
            "indent": [
                "error",
                4,
                {
                    "SwitchCase": 1
                }
            ],
            "no-console": 0,
            "generator-star-spacing": 0,
            "no-var": 1,
            "eqeqeq": [
                "error",
                "smart"
            ],
            "semi": [
                1,
                "always"
            ],
            "operator-linebreak": [
                2,
                "before",
                {
                    "overrides": {
                        "?": "after"
                    }
                }
            ],
            "no-debugger": 2,
            "quotes": [
                1,
                "single"
            ]
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "license": "UNLICENSED"
}